{
  "name": "@savoir/sdk",
  "version": "0.0.1",
  "description": "SDK for Savoir documentation AI with AI SDK compatible tools",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "ai": "^6.0.50",
    "evlog": "^1.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "ai": ">=6.0.50"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  }
}
