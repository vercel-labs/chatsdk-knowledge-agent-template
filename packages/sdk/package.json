{
  "name": "@savoir/sdk",
  "version": "0.1.0-alpha.1",
  "description": "SDK for Savoir documentation AI with AI SDK compatible tools",
  "license": "MIT",
  "author": "Hugo Richard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugorichard/savoir.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/hugorichard/savoir/tree/main/packages/sdk#readme",
  "bugs": {
    "url": "https://github.com/hugorichard/savoir/issues"
  },
  "keywords": [
    "savoir",
    "ai",
    "ai-sdk",
    "documentation",
    "rag",
    "sandbox"
